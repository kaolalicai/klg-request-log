{
  "name": "klg-request-log",
  "version": "2.0.1",
  "description": "koa log middware",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "lint": "tslint -c tslint.json 'src/**/*.ts' --fix",
    "build": "tsc -d -p ./",
    "test": "jest",
    "cov": "jest --coverage --coverageDirectory=coverage",
    "prepublishOnly": "npm run build",
    "pretest": "npm run lint"
  },
  "jest": {
    "verbose": true,
    "testURL": "http://localhost/",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "transform": {
      "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "/src/.*\\.(test|spec).(ts|tsx|js)$",
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,tsx,ts}",
      "!**/node_modules/**",
      "!**/vendor/**"
    ],
    "coverageReporters": [
      "json",
      "lcov"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kaolalicai/klg-request-log"
  },
  "keywords": [
    "klg"
  ],
  "author": "nick",
  "license": "MIT",
  "engines": {
    "node": ">=8.9.0"
  },
  "dependencies": {
    "klg-log-model": "^1.0.1",
    "klg-logger": "^2.0.1",
    "lodash": "^4.17.10",
    "moment": "^2.22.2",
    "tracer": "^0.8.15"
  },
  "devDependencies": {
    "@types/jest": "^22.2.3",
    "@types/koa": "^2.0.46",
    "@types/supertest": "^2.0.5",
    "jest": "^22.4.4",
    "koa": "^2.5.2",
    "koa-bodyparser": "^4.2.1",
    "koa-router": "^7.4.0",
    "mongoose": "^5.2.8",
    "source-map-support": "^0.5.8",
    "supertest": "^3.1.0",
    "ts-jest": "^22.4.6",
    "ts-node": "^5.0.1",
    "tslint": "^5.11.0",
    "tslint-config-klg": "^1.0.4",
    "typescript": "^2.9.2",
    "webstorm-disable-index": "^1.2.0"
  }
}
